{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "leftToast": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "subtitle": {
          "type": "string"
        }
      }
    },
    "rightToast": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "subtitle": {
          "type": "string"
        }
      }
    },
    "bottomToast": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "show": {
      "type": "string"
    }
  },
  "required": ["leftToast", "rightToast", "bottomToast"]
}
